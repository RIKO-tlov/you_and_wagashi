<div class="containe text-center">
  <div class="row">
    <div class="col-sm-12 mt-3">
      <h1>レビュー投稿編集</h1>
    </div>
  </div>
  <%= render 'public/shops/main', shop: @shop %>
  <%= form_with(model:[@shop, @review], :url => {:action => :update}, local: true) do |f| %>
  <%= render 'public/reviews/review_form', f: f %>
  <div class="row">
  <div class="col-sm-12">
    <%= f.submit '編集を保存', class:"btn-flat-logo mt-3" %>
  </div>
  </div>
  <% end %>
</div>